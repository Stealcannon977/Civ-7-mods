<Database>
	<Types>
		<!-- Major Civs -->
		<Row Type="TRAIT_PREFIX_CIVNAME_ABILITY" Kind="KIND_TRAIT"/>
	</Types>
	<Kinds>
		<InsertOrIgnore KIND="KIND_TRAIT"/>
	</Kinds>
	<Civilizations>
		<!-- Full Civs -->
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" Name="LOC_CIVILIZATION_PREFIX_CIVNAME_NAME" FullName="LOC_CIVILIZATION_PREFIX_CIVNAME_FULLNAME" Description="LOC_CIVILIZATION_PREFIX_CIVNAME_DESCRIPTION" Adjective="LOC_CIVILIZATION_PREFIX_CIVNAME_ADJECTIVE" StartingCivilizationLevelType="CIVILIZATION_LEVEL_FULL_CIV" UniqueCultureProgressionTree="TREE_CIVICS_AQ_PREFIX_CIVNAME" CapitalName="LOC_CITY_NAME_PREFIX_CIVNAME1" RandomCityNameDepth="10"/>
	</Civilizations>
	<CivilizationFavoredWonders>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" FavoredWonderType="WONDER_PREFIX_WONDERNAME" FavoredWonderName="LOC_WONDER_PREFIX_WONDERNAME_NAME"/>
	</CivilizationFavoredWonders>
	<Traits>
		<!--Civ Traits-->
		<Row TraitType="TRAIT_PREFIX_CIVNAME_ABILITY" Name="LOC_TRAIT_PREFIX_CIVNAME_ABILITY_NAME" Description="LOC_TRAIT_PREFIX_CIVNAME_ABILITY_DESCRIPTION" InternalOnly="true"/>
	</Traits>
	<TraitModifiers>
		<Row TraitType="TRAIT_PREFIX_CIVNAME_ABILITY" ModifierId="MOD_CIV_WONDER_PRODUCTION_PREFIX_CIVNAME"/>
    <Row TraitType="TRAIT_PREFIX_CIVNAME_ABILITY" ModifierId="TRAIT_PREFIX_CIVNAME_MOD_CITY_HALL_YIELDS"/>
	</TraitModifiers>
	<CivilizationTraits>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ANTIQUITY_CIV"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_PREFIX_CIVNAME"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_PREFIX_CIVNAME_ABILITY"/>
		<!-- Choose two below-->
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_CULTURAL"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_POLITICAL"/> <!-- This is Diplomatic -->
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_ECONOMIC"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_EXPANSIONIST"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_MILITARISTIC"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TraitType="TRAIT_ATTRIBUTE_SCIENTIFIC"/>
	</CivilizationTraits>
	<RequirementSets>
		<Row RequirementSetId="REQSET_PLAYER_IS_PREFIX_CIVNAME" RequirementSetType="REQUIREMENTSET_TEST_ALL"/>
	</RequirementSets>
	<RequirementSetRequirements>
		<Row RequirementSetId="REQSET_PLAYER_IS_PREFIX_CIVNAME" RequirementId="REQ_PLAYER_HAS_TRAIT_PREFIX_CIVNAME"/>
	</RequirementSetRequirements>
	<Requirements>
		<Row RequirementId="REQ_PLAYER_HAS_TRAIT_PREFIX_CIVNAME" RequirementType="REQUIREMENT_PLAYER_HAS_CIVILIZATION_OR_LEADER_TRAIT"/>
	</Requirements>
	<RequirementArguments>
		<Row RequirementId="REQ_PLAYER_HAS_TRAIT_PREFIX_CIVNAME" Name="TraitType" Value="TRAIT_PREFIX_CIVNAME"/>
	</RequirementArguments>
	<LeaderCivPriorities>
		<!--This is the table used for AI, the UI uses LeaderCivilizationBias in config.xml-->
		<Row Leader="LEADER_JOSE_RIZAL" Civilization="CIVILIZATION_PREFIX_CIVNAME" Priority="4"/>
	</LeaderCivPriorities>
	<CityNames>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" CityName="LOC_CITY_NAME_PREFIX_CIVNAME1"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" CityName="LOC_CITY_NAME_PREFIX_CIVNAME2"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" CityName="LOC_CITY_NAME_PREFIX_CIVNAME3"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" CityName="LOC_CITY_NAME_PREFIX_CIVNAME4"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" CityName="LOC_CITY_NAME_PREFIX_CIVNAME5"/>
		<!-- You can add more cities. Most Civs have 30-40 city names. -->
	</CityNames>
	<!-- Higher Score = More Likely -->
	<StartBiasBiomes>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" BiomeType="BIOME_TROPICAL" Score="20"/>
		<!-- DESERT, GRASSLAND, TROPICAL, MARINE, PLAINS, TUNDRA -->
	</StartBiasBiomes>
	<StartBiasTerrains>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" TerrainType="TERRAIN_COAST" Score="30"/>
		<!-- MOUNTAIN, HILL, FLAT, COAST, OCEAN, NAVIGABLE_RIVER -->
	</StartBiasTerrains>
	<VisArt_CivilizationBuildingCultures>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" BuildingCulture="BUILDING_CULTURE_PAC"/>
		<!-- BUILDING_CULTURE_[choice]: NAF, MED, EAS, PAC, MID, SAM, NAM -->
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" BuildingCulture="ANT_MUD"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" BuildingCulture="EXP_MUD"/>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" BuildingCulture="MOD_MUD"/>
		<!-- ANT/EXP/MOD_MUD, ANT/EXP/MOD_STONE -->
	</VisArt_CivilizationBuildingCultures>
	<VisArt_CivilizationUnitCultures>
		<Row CivilizationType="CIVILIZATION_PREFIX_CIVNAME" UnitCulture="Asian"/>
		<!-- Afr, Med, Asian, MidE, SAmer, NAmer -->
	</VisArt_CivilizationUnitCultures>

	<EndGameMovies>
	</EndGameMovies>
</Database>